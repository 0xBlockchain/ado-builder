{
    "schema": {
        "royalties": {
            "title": "Setup Royalties",
            "description": "Fees paid to the author, owner of the NFT. By percentage or flat fee.",
            "$id": "/schemas/royalties",
            "type": "object",
            "ui:panel": true,
            "ui:toggle": true,
            "properties": {
              "enabled": {
                "type": "boolean",
                "default": false,
                "ui:toggle": true
              },
              "description": {
                "type": "string",
                "title": "Royalty Description",
                "description": "*Optional - Information is solely for organizational use."
              },
              "distribution-type": {
                "type": "object",
                "title": "Distribution Type",
                "anyOf": [
                  {
                    "title": "By Percentage",
                    "type": "object",
                    "ui:group": true,
                    "properties": {
                      "distribution_percentage": {
                        "title": "% fee of sale price",
                        "type": "number"
                      }
                    }
                  },
                  {
                    "title": "By Flat Fee",
                    "type": "object",
                    "ui:group": true,
                    "properties": {
                      "distribution_fiat-fee": {
                        "type": "number",
                        "title": "Flat fee"
                      },
                      "distribution_fiat-fee-currency": {
                        "type": "string",
                        "title": "Currency type",
                        "enum": ["usd"],
                        "enumNames": ["USD"],
                        "default": "usd"
                      }
                    }
                  }
                ]
              },
              "payment_address": {
                "type":"array",
                "items": {
                  "type": "string"
                },
                "title": "Alphanumeric Terra Address",
                "description": "Address for royalty distributions to be provided."
              }
            }
          }
    },
    "ui-schema": {

    },
    "formdata": {
        
    }
}